<c74object name="rs.psk~" module="msp" category="MSP Control">
	<digest>
		Converts arbitrary binary data into phase-shift keying control signals.
	</digest>
	<description>
		<o>rs.psk~</o> by Reilly Spitzfaden can import any file as raw binary data and play this data back as a control signal for synthesizing PSK (phase-shift keying) telecommunications signals.
	</description>
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">
			Reilly Spitzfaden
		</metadata>
		<metadata name="tag">
			Max
		</metadata>
		<metadata name="tag">
			MSP Control
		</metadata>
		<metadata name="reference">
			TEXT_HERE
		</metadata>
		<metadata name="license">
			TEXT_HERE
		</metadata>
	</metadatalist>
	<!--INLETS-->
	<inletlist>
		<inlet id="0" type="signal">
			<digest>
				trigger inlet
			</digest>
			<description>
				Signal input: a single sample click advances the output to the next bit in the source file
			</description>
		</inlet>
	</inletlist>
	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="signal">
			<digest>
				Signal output
			</digest>
			<description>
			</description>
		</outlet>
	</outletlist>
	<!--ARGUMENTS-->
	<objarglist>
		<objarg name="file" optional="1" type="symbol">
			<digest>
				File to be imported
			</digest>
			<description>
				Name of the file to be imported
			</description>
		</objarg>
	</objarglist>
	<!--MESSAGES-->
	<methodlist>
		<method name="read">
			<arglist>
				<arg name="file name" optional="1" type="symbol" />
			</arglist>
			<digest>
				Load a file
			</digest>
			<description>
				"read" followed by a file name opens that file; "read" alone opens the file picker window.
			</description>
		</method>
	</methodlist>
